{
  "name": "io.tomba.verifier",
  "version": "1.0.0",
  "releasedAt": "July 1, 2023",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "add-android": "npx cap add android",
    "add-ios": "npx cap add ios",
    "open-android": "npx cap open android",
    "open-ios": "npx cap open ios",
    "sync-android": "npx cap sync android",
    "sync-ios": "npx cap sync ios",
    "run-android": "npx cap run android --verbose --sdk-info",
    "run-ios": "npx cap run ios --verbose --sdk-info",
    "run-assets": "npx @capacitor/assets generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "clean:dist": "rm -rf dist/ android/",
    "clean:node_modules": "rm -rf node_modules/ yarn.lock",
    "clean:all": "yarn clean:dist && yarn clean:node_modules",
    "lint": "yarn lint:js"
  },
  "dependencies": {
    "@capacitor/android": "^5.1.0",
    "@capacitor/core": "^5.1.0",
    "@capacitor/ios": "^5.1.0",
    "@mdi/font": "^7.2.96",
    "@nuxtjs/axios": "^5.13.6",
    "nuxt": "^2.14.0",
    "vuetify-loader": "^1.6.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^5.1.0",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "sass": "^1.45.1",
    "sass-loader": "^10.1.1"
  }
}
